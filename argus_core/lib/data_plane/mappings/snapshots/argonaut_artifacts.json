{
  "index": "argonaut_artifacts",
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0"
    }
  },
  "mappings": {
    "dynamic": false,
    "date_detection": false,
    "_meta": {
      "argonaut_mapping_version": "1.0"
    },
    "properties": {
      "artifactId": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "runId": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "repo": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "buildId": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "type": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "sourceTool": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "filename": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "checksum": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "ingestStatus": {
        "type": "keyword",
        "doc_values": true,
        "index": true,
        "norms": false
      },
      "timestamp": {
        "type": "date"
      }
    }
  }
}
