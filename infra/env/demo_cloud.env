# Elastic Cloud (required)
ES_URL="https://<your-es-endpoint>:443"
ES_API_KEY="<set-in-local-secrets-file>"

# Kibana (required for saved objects import + links)
KIBANA_URL="https://<your-kibana-endpoint>:443"

# One of these must be set for Kibana auth. Pick ONE and lock it:
KIBANA_API_KEY="<set-in-local-secrets-file>"
# OR:
# KIBANA_USERNAME="elastic"
# KIBANA_PASSWORD="<set-in-local-secrets-file>"

# Demo mode lock
DEMO_ENV="cloud"
EXECUTION_MODE="es"

# Object store (Task 6.15)
DEMO_OBJECTSTORE_ENDPOINT="https://us-east-1.linodeobjects.com"
DEMO_OBJECTSTORE_BUCKET="argonaut"
DEMO_OBJECTSTORE_PREFIX="bundles"
DEMO_OBJECTSTORE_ACCESS_KEY_ID="<set-in-local-secrets-file>"
DEMO_OBJECTSTORE_SECRET_ACCESS_KEY="<set-in-local-secrets-file>"

# Optional demo defaults
DEMO_REPO="payment-service"
DEMO_BUILD_ID="128"
DEMO_BUNDLE_PATH="demo-data/bundles/payment-service_build-128"
